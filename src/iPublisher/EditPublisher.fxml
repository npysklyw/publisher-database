<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iPublisher.EditPublisherController">
   <children>
      <Label layoutX="206.0" layoutY="40.0" text="Edit/Delete A Publisher ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="486.0" layoutY="343.0" mnemonicParsing="false" onAction="#update" text="Update" />
