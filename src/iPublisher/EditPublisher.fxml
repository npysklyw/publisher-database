      <Label layoutX="178.0" layoutY="102.0" text="Get Publisher" />
      <TextField fx:id="name" layoutX="273.0" layoutY="160.0" />
      <TextField fx:id="address" layoutX="273.0" layoutY="209.0" />
      <TextField fx:id="region" layoutX="273.0" layoutY="254.0" />
      <Label layoutX="198.0" layoutY="164.0" text="Name" />
      <Label layoutX="192.0" layoutY="213.0" text="Address" />
      <Label layoutX="193.0" layoutY="258.0" text="Region" />
   </children>
</AnchorPane>
